
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'himawari_marcet_info_ipv4'
	option service_name 'namecheap.com'
	option enabled '1'
	option domain 'home'
	option username 'marcet.info'
	option password 'b27d9d50528d446585a3da5a1a5435c7'
	option use_https '1'
	option dns_server '9.9.9.9'
	option use_syslog '1'
	option lookup_host 'himawari.marcet.info'

