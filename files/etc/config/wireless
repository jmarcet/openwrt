
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'soc/1b500000.pci/pci0000:00/0000:00:00.0/0000:01:00.0'
	option country 'US'
	option channel '44'
	option legacy_rates '0'
	option htmode 'VHT80'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'nighthawk-plus'
	option network 'lan'
	option encryption 'psk2+ccmp'
	option key '@Ed#qd$dsg%sed&eq(mm)'
	option wpa_disable_eapol_key_retries '1'
	option wps_pushbutton '0'

config wifi-iface 'enterprise_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'nighthawk-plus-plus'
	option network 'lan'
	option encryption 'wpa2+ccmp'
	option wps_pushbutton '0'
	option auth_server '127.0.0.1'
	option auth_secret 'hohT9Kohgh4muf7r'

config wifi-iface 'guests_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'nighthawk-plus-invitados'
	option network 'lan'
	option encryption 'psk2+ccmp'
	option key '_cocapepalanamini-'
	option isolate '1'
	option wpa_disable_eapol_key_retries '1'
	option wps_pushbutton '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11g'
	option path 'soc/1b700000.pci/pci0001:00/0001:00:00.0/0001:01:00.0'
	option channel '4'
	option country 'US'
	option legacy_rates '0'
	option htmode 'HT40'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'nighthawk'
	option network 'lan'
	option encryption 'wpa2+ccmp'
	option wps_pushbutton '0'
	option auth_server '127.0.0.1'
	option auth_secret 'hohT9Kohgh4muf7r'

config wifi-iface 'guests_radio1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'nighthawk-invitados'
	option network 'lan'
	option encryption 'psk2+ccmp'
	option key '_cocapepalanamini-'
	option isolate '1'
	option wpa_disable_eapol_key_retries '1'
	option wps_pushbutton '0'

